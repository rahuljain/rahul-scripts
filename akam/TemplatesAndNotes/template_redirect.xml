<match:hoit host="www.espanol.mybestbuy.com espanol.mybestbuy.com">
  <edgeservices:redirect.generate.evict-cached-object>off</edgeservices:redirect.generate.evict-cached-object>
  <edgeservices:redirect.generate.rule name="espanol.mybestbuy.com">
    <response-code>302</response-code>
    <select-prefix>/</select-prefix>
    <destination>=%(AK_SCHEME)://espanol.my.bestbuy.com%(AK_URL)</destination>
    <status>on</status>
  </edgeservices:redirect.generate.rule>
</match:hoit>

<match:uri.wildcard value="/OnlineMall /OnlineMall/" case="off">
 <edgeservices:redirect.generate.evict-cached-object>off</edgeservices:redirect.generate.evict-cached-object>
 <edgeservices:redirect.generate.rule name="OnlineMall redirect">
   <response-code>302</response-code>
   <select-prefix>/</select-prefix>
   <destination>=https://%(AK_HOST)/offers/onlinemall</destination>
   <status>on</status>
 </edgeservices:redirect.generate.rule>
</match:uri.wildcard>